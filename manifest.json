{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "get_started32.png",
    "default_popup": "./public/scripts/index.html",
    "default_title": "A popup will come here."
  }
}
